{
    "name": "wireui/breadcrumbs",
    "description": "Breadcrumbs component for Laravel, Livewire, TallStack",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PH7-Jack",
            "email": "pedrolivertwd@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0",
        "livewire/livewire": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0",
        "nunomaduro/larastan": "^2.5",
        "laravel/pint": "^1.9"
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "./vendor/bin/pest --coverage --min=100",
        "phpstan": "./vendor/bin/phpstan analyse",
        "pint:fix": "./vendor/bin/pint",
        "pint:check": "./vendor/bin/pint --test",
        "verify": [
            "@pint:check",
            "@phpstan"
        ]
    },
    "autoload": {
        "psr-4": {
            "WireUi\\Breadcrumbs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WireUi\\Breadcrumbs\\ServiceProvider"
            ],
            "aliases": []
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
