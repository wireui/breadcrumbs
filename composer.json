{
    "name": "wireui/breadcrumbs",
    "type": "library",
    "description": "Breadcrumbs component for Laravel, Livewire, TallStack",
    "license": "MIT",
    "keywords": [
        "wireui",
        "livewire-ui",
        "blade components",
        "laravel components",
        "livewire components"
    ],
    "authors": [
        {
            "name": "Pedro Oliveira",
            "email": "pedro@wireui.dev"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0|^9.0",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "WireUi\\Breadcrumbs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --coverage --min=100 --parallel",
        "phpstan": "./vendor/bin/phpstan analyse",
        "pint:fix": "./vendor/bin/pint",
        "pint:check": "./vendor/bin/pint --test",
        "verify": [
            "@pint:check",
            "@phpstan"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "WireUi\\Breadcrumbs\\ServiceProvider"
            ],
            "aliases": []
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
